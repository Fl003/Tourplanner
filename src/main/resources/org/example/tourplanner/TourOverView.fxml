<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.tourplanner.view.TourListController"
            minWidth="250.0" style="-fx-background-color: white">
        <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" style="&#10;"
               text="%ListViewTitle" textFill="#70b152">
            <font>
                <Font name="Arial Bold" size="18.0"/>
            </font>
        </Label>
        <ListView fx:id="tourList" layoutX="11.0" layoutY="44.0" prefHeight="466.0" prefWidth="230.0"
                  AnchorPane.bottomAnchor="49.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
                  AnchorPane.topAnchor="44.0"/>
        <Button layoutX="11.0" layoutY="513.0" mnemonicParsing="false" prefHeight="38.0"
                prefWidth="200.0" text="%Tour_Create" textFill="WHITE" AnchorPane.bottomAnchor="10.0"
                AnchorPane.leftAnchor="11.0" AnchorPane.rightAnchor="10.0" onAction="#showTourModal">
            <font>
                <Font name="Arial Bold" size="12.0"/>
            </font>
        </Button>
</AnchorPane>
